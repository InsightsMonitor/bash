{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "interface-name": [true, "always-prefix"],
    "no-string-literal": false,
    "object-literal-key-quotes": [true, "as-needed"],
    "prefer-for-of": false,
    "semicolon": [true, "never"]
  }
}
